squares = [x**2 for x in range(5)]
print(squares)
even_numbers= [x for x in range (10)if x%2==0]
print(even_numbers)
matrix=[i * j for j in range(1,4) for i in range(1,4)]
print(matrix)
name=["alice","bob","sway"]
uppercased_names=[name.upper() for name in name]
print(uppercased_names)
nested_list=[[1,2],[2,3],[3,4],[5,6]]
flat_list=[item for sublist in nested_list for item in sublist]
print(flat_list)
sentence="python is a fantastic language to practise coding"
vowels="aeiou"
result=[
    word for word in sentence.lower().split()
        if sum(1 for char in word if char in vowels) > len(word) -sum(1 for char in word if char in vowels)]
print(result)
highest_divisors={
    num:max([
        d for d in range (2,10) if num % d==0
    ],default=None)
    for num in range (1,1001)
}
print(f"highest_divisor for 10 is {highest_divisors.get(10)}")